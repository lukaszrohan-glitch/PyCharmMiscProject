{
  "name": "arkuszownia-smb-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "vite build",
    "lint": "cross-env ESLINT_USE_FLAT_CONFIG=true eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "cross-env ESLINT_USE_FLAT_CONFIG=true eslint src --ext js,jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,css,json}\"",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:a11y": "vitest --run --testNamePattern=accessibility",
    "test:e2e": "npx playwright test"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@tanstack/react-query": "^5.90.12",
    "axios": "^1.5.0",
    "classnames": "^2.5.1",
    "date-fns": "^2.30.0",
    "framer-motion": "^12.23.25",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.30.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/plugin-syntax-jsx": "^7.22.5",
    "@babel/plugin-transform-react-jsx": "^7.22.15",
    "@babel/plugin-transform-runtime": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/runtime": "^7.23.2",
    "@playwright/test": "^1.48.2",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/ui": "^4.0.14",
    "axe-core": "^4.8.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.11.0",
    "jest-axe": "^8.0.0",
    "jsdom": "^27.2.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.10",
    "vite": "^7.2.2",
    "vitest": "^4.0.14"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
